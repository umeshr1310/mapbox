<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

   <com.mapbox.mapboxsdk.maps.MapView
       android:id="@+id/mapView"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       mapbox:mapbox_cameraBearing="20"
       mapbox:mapbox_cameraTargetLat="25.7899197"
       mapbox:mapbox_cameraTargetLng="-80.12952"
       mapbox:mapbox_cameraTilt="40"
       mapbox:mapbox_cameraZoom="13.99"
       mapbox:mapbox_styleUrl="@string/mapbox_style_dark" />

</android.support.constraint.ConstraintLayout>